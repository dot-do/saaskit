{
  "name": "@saaskit/headless",
  "version": "0.0.1",
  "description": "Headless SaaS builder for AI Agents - Nouns, Verbs, Actions, Events",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "headless",
    "saas",
    "ai",
    "agents",
    "nouns",
    "verbs",
    "actions",
    "events"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  }
}
